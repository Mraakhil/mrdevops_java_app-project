@Library('my-shared-library') _

pipeline{

    agent any
    tools {
        
        maven 'maven3.9.9'   // Your Maven tool name configured in Jenkins
    }

    stages{
         
        stage('Git Checkout'){

            steps{
                 script{
                     gitCheckout(
                     branch: "main",
                     url: "https://github.com/imranmycloud/mrdevops_java_app-project.git"
             )
             
                }
            }
        }
         stage('Unit Test maven'){

            steps{
                 script{
                   mvnTest()
             
                }
            }
        }
    }
}
