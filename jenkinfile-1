@Library('my-shared-library') _

pipeline {
    agent any 

    stages {
        stage('Git Checkout') {     
            steps {
                script {
                    gitCheckout(
                        branch: "main",
                        url: "https://github.com/imranmycloud/mrdevops_java_app-project.git"
                    )
                }
            }
        }

        stage('Integration Test maven') {     
            steps {
                script {
                    mvnIntegrationTest.groovy()
                }
            }
        }
    }
}
